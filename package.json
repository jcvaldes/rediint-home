{
  "name": "rediint-home",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng s -o --hmr --host=0.0.0.0 --proxy-config ./proxy.json --disable-host-check --ssl true --ssl-key src/assets/sslcertificate/server.key --ssl-cert src/assets/sslcertificate/server.crt",
    "start:prod": "browser-sync start --server 'dist/rediint-home' -w --files \"dist/rediint-home\"",
    "start:es": "ng s -o --configuration=es",
    "start:en": "ng s -o --configuration=en --hmr",
    "build": "ng build",
    "build:es": "ng build --configuration=es",
    "build:all": "ng build --prod && ng build --configuration=es",
    "build:deploy": "ng build --prod && ng build --configuration=es && firebase deploy",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "hmr": "ng serve --hmr -e=hmr",
    "int:extract": "ng xi18n --output-path src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/localize": "^13.1.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "browser-sync": "^2.27.7",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}
